{
  "version": 2,
  "public": false,
  "alias": ["www.api.kthkit.se", "api.kthkit.se"],
  "name": "KTHKIT API",
  "builds": [
    {
      "src": "profile.js",
      "use": "@now/node",
      "config": { "maxLambdaSize": "40mb" }
    },
    {
      "src": "grades.js",
      "use": "@now/node",
      "config": { "maxLambdaSize": "40mb" }
    }
  ],
  "routes": [
    { "src": "/profile/(.*)", "dest": "/profile.js" },
    {
      "src": "/grades/(.*)",
      "dest": "/grades.js"
    }
  ]
}
